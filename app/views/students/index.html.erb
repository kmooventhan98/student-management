<div class="container-xl">
	<h1 class="text-center mt-2">All students</h1>
	<table class="table table-striped">
		<tr>
			<th> First Name</th>	
			<th> Last Name</th>	
			<th> Email Name.</th>	
		</tr>
		<% @students.each do |student| %>
			<tr>
				<td> <%= student.first_name%> </td>
				<td> <%= student.last_name%> </td>
				<td> <%= student.email%> </td>
			</tr>
		<% end %>
	</table>
</div>